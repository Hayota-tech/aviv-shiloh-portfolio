{
  "name": "aviv-shiloh-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start:dev": "next dev -p 80",
    "build": "next build && next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,scss,js,json}\"",
    "type-check": "tsc --noEmit",
    "depcheck:root": "depcheck --config ./.depcheckrc.json",
    "template:page": "hygen component new-page",
    "template:container": "hygen component new-container",
    "template:layout": "hygen component new-layout",
    "template:ui": "hygen component new-ui"
  },
  "dependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.3"
  },
  "devDependencies": {
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "depcheck": "^1.4.3",
    "eslint": "8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-i18next": "^6.0.0-4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "hygen": "^6.2.11",
    "prettier": "^2.5.1",
    "prettier-plugin-sh": "^0.12.8",
    "sass": "^1.49.7",
    "stylelint": "^14.5.0",
    "typescript": "4.5.5"
  }
}
